      speaker: 3
      line: Oh f-
    - id: jBr4
      speaker: 0
      line: '*Scared dog noises*'
    - id: jBr5
      speaker: 3
      line: Jack...
    - id: jBr6
      speaker: 0
      line: '*scared dog noises*'
